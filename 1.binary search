// User function Template for Java
class Solution {
    public ArrayList<Integer> firstAndLast(int x, int arr[]) {
        // Code here
        int first=-1; 
        int last=-1;
        int low=0,high=arr.length-1;
        ArrayList<Integer> al=new ArrayList<>(); 
        while(low<=high){ 
             int mid=low+(high-low)/2;
            if(arr[mid]==x){
                first=mid;
                high=mid-1;
            }
            else if(arr[mid]>x){
                high=mid-1;
            }
            else{
                low=mid+1;
            }
        }
        low=0;
        high=arr.length-1;
        while(low<=high){
            int mid=low+(high-low)/2;
            if(arr[mid]==x){
                last=mid;
                low=mid+1;
            }
            else if(arr[mid]>x){
                high=mid-1;
            }
            else{
                low=mid+1;
            }
        }
        if(first!=-1 || last!=-1){
        if(first!=-1 ){
        al.add(first);
        }
        if(last!=-1){
            al.add(last);
        }
        }
       else{
        al.add(-1);
       }
        return al;
        
    }
}
