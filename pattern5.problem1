class Solution { 
    public double findMaxAverage(int[] nums, int k) {
        int windowsum=0;
        
        for(int i=0;i<k;i++){
            windowsum+=nums[i];
        }
        double maxsum=windowsum;
        for(int i=k;i<nums.length;i++){
            windowsum=(windowsum+nums[i]-nums[i-k]);
            maxsum=Math.max(windowsum,maxsum);
        }
        return maxsum/k;
    }
}
